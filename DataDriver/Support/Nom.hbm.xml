<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DataDriver"
                   namespace="DataDriver"
                   default-lazy="false">
  <class name="Nom" table="Noms">

    <id name="Id" column="NomKey" type="int" unsaved-value="0" >
      <generator class="increment"/>
    </id>

    <property name="Name" column="Name" not-null="false" type="System.String" insert="true" update="true" />
    <property name="CaloriesPerGram" column="CaloriesPerGram" not-null="false" type="System.Double" insert="true" update="true" />
    <property name="Gushy" column="Gushy" not-null="false" type="System.Boolean" insert="true" update="true" />

  </class>
</hibernate-mapping>

